{
  "expo": {
    "name": "Realst Izzi",
    "slug": "realst-izzi",
    "owner": "kmc-savills",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "extra": {
      "clerkPublishableKey": "pk_test_Z2xhZC1ibG93ZmlzaC0yMS5jbGVyay5hY2NvdW50cy5kZXYk",
      "apiUrl": "https://izzi-serverless-api.kmc-savills-inc-account.workers.dev",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "edca28f9-56f5-4627-83c5-b82f891ffb4b"
      }
    },
    "splash": {
      "image": "./assets/images/splash.png",
