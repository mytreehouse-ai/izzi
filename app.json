{
  "expo": {
    "name": "Izzi",
    "slug": "izzi",
    "owner": "kmc-savills",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "extra": {
      "clerkPublishableKey": "pk_test_Z2xhZC1ibG93ZmlzaC0yMS5jbGVyay5hY2NvdW50cy5kZXYk",
      "apiUrl": "https://izzi-serverless-api.kmc-savills-inc-account.workers.dev",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "030d5b18-9359-4beb-a83e-e1bac4bbd933"
      }
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "izzi.com",
      "supportsTablet": true
    },
    "android": {
      "package": "com.kmcsavills.izzi",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Izzi to use your location for tailored property recommendations."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/030d5b18-9359-4beb-a83e-e1bac4bbd933"
    }
  }
}
